{

    "description": "Block annoying mailtracking",
    "manifest_version": 2,
    "name": "Block Mail Track",
    "version": "1.0",
  
    "permissions": [
      "webRequest",
      "webRequestBlocking",
      "https://*.googleusercontent.com/",
      "https://mail.google.com/*"
    ],
   
    "background": {
      "scripts": ["background.js"]
    },
 
  "browser_action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png"
    }
  }
  
  }